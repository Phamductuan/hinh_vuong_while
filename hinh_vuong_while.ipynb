   "source": [
    "import turtle\n",
    "a = int(input(\"nhap do dai cac canh hinh vuong: \"))\n",
    "t= turtle. Turtle()\n",
    "t.fillcolor(\"red\")\n",
    "t.begin_fill()\n",
    "edge = 0\n",
    "while edge <= 4:\n",
    "    edge += 1\n",
    "    t.fd(a)\n",
    "    t.lt(90)\n",
    "t.end_fill()\n",
    "turtle.done()\n"
   ]
  }
